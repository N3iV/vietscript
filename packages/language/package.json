{
  "name": "viscript",
  "version": "0.0.1",
  "description": "Vietnamese programming language - Ngôn ngữ lập trình tiếng Việt.",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "types": "./dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "typecheck": "tsc --noEmit",
    "build": "rimraf ./dist && vite build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/imrim12/viscript.git",
    "directory": "language"
  },
  "keywords": [
    "language",
    "viscript",
    "vietnamese",
    "programming language"
  ],
  "author": "thecodeorigin",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/imrim12/viscript/issues"
  },
  "homepage": "https://github.com/imrim12/viscript#readme",
  "dependencies": {
    "@viscript/shared": "workspace:*"
  },
  "devDependencies": {
    "@vitest/ui": "^0.24.5",
    "rimraf": "^3.0.2",
    "typescript": "^4.9.4",
    "vite": "^3.1.3",
    "vite-plugin-dts": "^1.6.6",
    "vitest": "^0.24.4"
  }
}
